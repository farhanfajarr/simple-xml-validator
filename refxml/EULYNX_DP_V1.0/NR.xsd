  <xs:import namespace="http://dataprep.eulynx.eu/schema/Generic/1.0" schemaLocation="Generic.xsd"/>
  <xs:import namespace="http://dataprep.eulynx.eu/schema/Generic/1.0" schemaLocation="Generic.xsd"/>
  <xs:import namespace="http://dataprep.eulynx.eu/schema/Signalling/1.0" schemaLocation="Signalling.xsd"/>
  <xs:import namespace="http://www.railsystemmodel.org/schemas/Common/1.2" schemaLocation="RsmCommon.xsd"/>
  <xs:complexType name="AtpBeacon">
    <xs:annotation>
      <xs:documentation>A beacon in the ATP system. </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:TpsSpotDevice"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="LightSignal">
    <xs:annotation>
      <xs:documentation>NR Light signal</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:LightSignal">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="hasBackBoard" type="nr:BackBoardTypes"/>
          <xs:element maxOccurs="1" minOccurs="0" name="hasHoodLength" type="nr:HoodsLengthTypes"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="Designation">
    <xs:complexContent>
      <xs:extension base="generic:Designation">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="1" name="longName" nillable="true" type="xs:string"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="LensDiffuserWithHotStrip">
    <xs:complexContent>
      <xs:extension base="nr:LensDiffuser">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="1" name="value" nillable="true" type="xs:string">
            <xs:annotation>
              <xs:documentation>The hot strip indicates the direction of the focus of the light beam and is given in terms like  "left upper quadrant (LUQ)" or "5 o'clock ".</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="OverheadLineMastPairingTypes">
    <xs:restriction base="xs:string">
      <xs:enumeration value="headSpan">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="gantry">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="other">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="RoadTrafficSign">
    <xs:annotation>
      <xs:documentation>Road (traffic) light.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:RoadTrafficSign">
        <xs:sequence>
          <xs:element name="isOfRtlType" nillable="true" type="nr:RtlTypes"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="SignalFrame">
    <xs:annotation>
      <xs:documentation>NR signal frame.
The type of frame is inherent to the aspect, i.e. speed warning, termination, repeater, spate, enhanced permissible speed.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:SignalFrame">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="hasComponent" type="nr:SignalFrameComponent"/>
          <xs:element name="hasFrameDimension" nillable="true" type="nr:FrameDimension"/>
          <xs:element maxOccurs="1" minOccurs="0" name="hasMountingPoint" type="nr:GridLocation"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="TractionFeedTypes">
    <xs:restriction base="xs:string">
      <xs:enumeration value="subStation">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="feederStation">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="trackParallelingHut">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="trackSectioningCabin">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="other">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="RailNodePath">
    <xs:annotation>
      <xs:documentation>Represents a path through a rail node (operational locality, station) by connecting two lines of way.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="1" name="capacityLimit" nillable="true" type="xs:decimal"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="LevelCrossingLCU">
    <xs:annotation>
      <xs:documentation>The local control unit of the level crossing.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:LocalOperationDevice">
        <xs:sequence>
          <xs:element name="isLocatedInQuadrant" nillable="true" type="sig:Quadrants"/>
          <xs:element name="worksLevelCrossingSystem" nillable="true" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>sig:LevelCrossingSystem</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="TpwsSystemProperties">
    <xs:annotation>
      <xs:documentation>TPWS system property set.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:TpsDataTransmissionSystemProperties">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="hasTpwsFunction" type="nr:TpwsFunctionTypes"/>
          <xs:element maxOccurs="unbounded" minOccurs="1" name="hasTpwsTrainCategorySpecificProperties" nillable="true" type="nr:TpwsTrainCategorySpecificProperties"/>
          <xs:element maxOccurs="1" minOccurs="0" name="isOfTpwsSystemType" type="nr:TpwsSystemTypes"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="TrainStop">
    <xs:annotation>
      <xs:documentation>Line-side mechanical device that activates the train's trip cock.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:TpsSpotDevice">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="type" type="xs:string"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="SignalBeyondVisibleTypes">
    <xs:annotation>
      <xs:documentation>Is the signal ahead visible?</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="yes">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="no">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="notApplicable">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="TripcockTester">
    <xs:annotation>
      <xs:documentation>Line-side device for testing the train-borne trip cock.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:TpsSpotDevice">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="type" type="xs:string">
            <xs:annotation>
              <xs:documentation>Informs about the powering.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EquipmentLocation">
    <xs:annotation>
      <xs:documentation>Defines the types of equipment location.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:ConfigurationProperty">
        <xs:sequence>
          <xs:element name="isOfEquipmentLocationType" nillable="true" type="nr:EquipmentLocationTypes"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="TrafficFlow">
    <xs:annotation>
      <xs:documentation>Defines fields for traffic flow modelling.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="PassengerLoadingPercentage" type="xs:decimal">
            <xs:annotation>
              <xs:documentation>The percentage passenger loading for the traffic flow (percentages above 100 are allowable).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="1" name="colour" nillable="true" type="xs:string">
            <xs:annotation>
              <xs:documentation>The colour of the label for the traffic flow in eight-character hexadecimal ARGB format.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="isLocatedAt" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>rsmCommon:SpotLocation</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="numberOfCars" type="xs:int">
            <xs:annotation>
              <xs:documentation>The number of cars in the trains for this traffic flow.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="perDayValue" type="xs:int">
            <xs:annotation>
              <xs:documentation>The number of trains past the point in the specified direction per day.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="perHourValue" type="xs:int">
            <xs:annotation>
              <xs:documentation>The number of trains past the point in the specified direction per peak hour.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="refersToTimingPoint" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Reference to the associated TIPLOC, if relevant.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>nr:TimingPoint</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="1" name="type" nillable="true" type="xs:string">
            <xs:annotation>
              <xs:documentation>The type of traffic flow (e.g. freight trains, high speed passenger trains).</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="AwsMagnet">
    <xs:annotation>
      <xs:documentation>Class for storing information about an AWS magnet.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:TpsSpotDevice">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="positionInGroup" type="xs:int">
            <xs:annotation>
              <xs:documentation>Position in group of the individual AWS magnet.

Increases in the track chainage direction from 1 onwards.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="type" type="xs:string">
            <xs:annotation>
              <xs:documentation>Can inform about
&lt;ul&gt;
 &lt;li&gt;strong magnet&lt;/li&gt;
&lt;/ul&gt;
&lt;ul&gt;
 &lt;li&gt;suppression&lt;/li&gt;
&lt;/ul&gt;</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="LockoutDevice">
    <xs:annotation>
      <xs:documentation>NR device
Patrol man lockout device (PLOD).</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:LockoutDevice">
        <xs:sequence>
          <xs:element name="isOfType" nillable="true" type="nr:LockoutDeviceTypes"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="ApcMagnet">
    <xs:annotation>
      <xs:documentation>Automatic Power Control Magnet.
Strictly speaking, this is not a train protection device but it uses the same technology to transmit information about the neutral section. </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:TpsSpotDevice">
        <xs:sequence>
          <xs:element name="appliesToNeutralSection" nillable="true" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>sig:ElectrificationGap</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="LevelCrossingSystem">
    <xs:annotation>
      <xs:documentation>NR level crossing system.
Note that it is not necessary to define the type or streetname of the protected track crossing, its spot location suffices.
</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:LevelCrossingSystem">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="isFittedWithFloodLight" type="nr:Floodlight">
            <xs:annotation>
              <xs:documentation>The level crossing system is fitted with a headlight unit.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="1" name="isOfLevelCrossingSystemType" nillable="true" type="nr:LevelCrossingControlTypes"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="FullDesignation">
    <xs:annotation>
      <xs:documentation>Used to be the signal box short code. Today, the line code is used.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="nr:Designation">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="1" name="name" nillable="true" type="xs:string"/>
          <xs:element maxOccurs="1" minOccurs="1" name="prefix" nillable="true" type="xs:string"/>
          <xs:element maxOccurs="1" minOccurs="0" name="suffix" type="xs:string">
            <xs:annotation>
              <xs:documentation>Used for special objects like banner or co-acting signals.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="RtlTypes">
    <xs:annotation>
      <xs:documentation>Types of road traffic light.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="msl_pedestrian">
        <xs:annotation>
          <xs:documentation>Miniature stop light, pedestrian (previously known as miniature warning lights).</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="msl_vehicular">
        <xs:annotation>
          <xs:documentation>Miniature stop light, vehicular (previously known as miniature warning lights).
</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="barrowCrossingLights">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="roadTrafficLight">
        <xs:annotation>
          <xs:documentation>Road traffic light; can have a St. Andrews cross.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="GiveWay">
        <xs:annotation>
          <xs:documentation>Give-way sign usually has a St Andrews cross.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="stAndrewsCrossOnly">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="other">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="PointHandle">
    <xs:annotation>
      <xs:documentation>The point or crank handle box stores equipment for manually operating and clamping points. </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="isHousedIn" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Reference to the housing, typically a cabinet, where equipment is stored. Housing is spot-located.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>sig:Housing</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="1" name="type" nillable="true" type="xs:string">
            <xs:annotation>
              <xs:documentation>Describes the type of equipment; CH for Point Crank Handle, PH for Pump Handle.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="TpwsFrequencyTypes">
    <xs:annotation>
      <xs:documentation>TPWS frequencies for arming and trigger loops. The loops are stated to be Normal or Opposite direction.
&lt;ol&gt;
 &lt;li&gt;f1 - 64.25 kHz&lt;/li&gt;
 &lt;li&gt;f2 - 65.25 kHz&lt;/li&gt;
 &lt;li&gt;f3 - 66.25 kHz&lt;/li&gt;
 &lt;li&gt;f4 - 64.75 kHz&lt;/li&gt;
 &lt;li&gt;f5 - 65.75 kHz&lt;/li&gt;
 &lt;li&gt;f6 - 66.75 kHz&lt;/li&gt;
&lt;/ol&gt;</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="f1">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="f2">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="f3">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="f4">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="f5">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="f6">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="f5_f6">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="SignalFrameComponent">
    <xs:annotation>
      <xs:documentation>Lamps or indicators that are part of a signal housing.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:SignalFrameComponent">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="hasGridLocation" type="nr:GridLocation"/>
          <xs:element maxOccurs="1" minOccurs="0" name="pitchAngle" type="xs:decimal">
            <xs:annotation>
              <xs:documentation>The pitch of the lamp on the component for SSiFT integration</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="yawAngle" type="xs:decimal">
            <xs:annotation>
              <xs:documentation>The yaw of the lamp on the component for SSiFT integration.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="PopGroup">
    <xs:annotation>
      <xs:documentation>Predicted Overrun Protection (POP) group of signals.
Overrun of any of the signals in the POP group triggers a closing of all signals.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:ConfiguredBaseObject"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="SubTimingPoint">
    <xs:annotation>
      <xs:documentation>The sub-TIPLOC gives more detail for a given TIPLOC code (such as where a line code is used with a TIPLOC code to define the path of a timetabled service)</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="nr:TimingPoint">
        <xs:sequence>
          <xs:element name="hasParentTimingPoint" nillable="true" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Reference to the TIPLOC that owns this sub-TIPLOC.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>nr:TimingPoint</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="OverheadLineMast">
    <xs:annotation>
      <xs:documentation>NR class for overhead lines carrying mast. 
Also known as stanchion (en-GB).</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:OverheadLineMast">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="isOfPairingType" type="nr:OverheadLineMastPairingTypes">
            <xs:annotation>
              <xs:documentation>Indicates the type of pairing with another mast.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="2" minOccurs="0" name="isPairedWithMast" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Allows pairing a mast with up to 2 other masts across the tracks. </xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>nr:OverheadLineMast</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="stanchionType" type="xs:string"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="TpwsTrainCategorySpecificProperties">
    <xs:complexContent>
      <xs:extension base="generic:BaseObject">
        <xs:sequence>
          <xs:element name="appliesToTrainCategoryForMaximumOverrunDistance" nillable="true" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>sig:TrainCategory</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="hasMaximumOverrunDistance" nillable="true" type="rsmCommon:Length"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="SignallingEquivalentUnit">
    <xs:annotation>
