<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:generic="http://dataprep.eulynx.eu/schema/Generic/1.0" xmlns:nr="http://dataprep.eulynx.eu/schema/NR/1.0" xmlns:rsmCommon="http://www.railsystemmodel.org/schemas/Common/1.2" xmlns:sig="http://dataprep.eulynx.eu/schema/Signalling/1.0" elementFormDefault="qualified" targetNamespace="http://dataprep.eulynx.eu/schema/NR/1.0">
