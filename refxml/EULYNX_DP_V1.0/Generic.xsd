Detailed technical information is found on https://dataprep.eulynx.eu
Please address queries to the contact stated on the EULYNX website https://eulynx.eu
</xs:documentation>
  </xs:annotation>
  <xs:import namespace="http://dataprep.eulynx.eu/schema/RFI/1.0" schemaLocation="RFI.xsd"/>
  <xs:import namespace="http://dataprep.eulynx.eu/schema/SNCF/1.0" schemaLocation="SNCF.xsd"/>
  <xs:import namespace="http://dataprep.eulynx.eu/schema/Signalling/1.0" schemaLocation="Signalling.xsd"/>
  <xs:import namespace="http://dataprep.eulynx.eu/schema/TRV/1.0" schemaLocation="TRV.xsd"/>
  <xs:import namespace="http://www.railsystemmodel.org/schemas/Track/1.2" schemaLocation="RsmTrack.xsd"/>
  <xs:import namespace="http://dataprep.eulynx.eu/schema/DB/1.0" schemaLocation="DB.xsd"/>
  <xs:import namespace="http://www.railsystemmodel.org/schemas/Signalling/1.2" schemaLocation="RsmSignalling.xsd"/>
  <xs:import namespace="http://dataprep.eulynx.eu/schema/NR/1.0" schemaLocation="NR.xsd"/>
  <xs:import namespace="http://dataprep.eulynx.eu/schema/ProRail/1.0" schemaLocation="ProRail.xsd"/>
  <xs:import namespace="http://www.railsystemmodel.org/schemas/Common/1.2" schemaLocation="RsmCommon.xsd"/>
  <xs:complexType name="Topography">
    <xs:annotation>
      <xs:documentation>Container for topography, i.e. position and alignment components. 
Coordinates are typically geographic but may include paper and display coordinates.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:BaseObject">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="usesAlignmentCantSegment" type="rsmCommon:AlignmentCantSegment"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="usesAlignmentCurve" type="rsmCommon:AlignmentCurve"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="usesAnchorPoint" type="rsmCommon:AnchorPoint"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="usesCant" type="rsmCommon:Cant"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="usesDiscretisedPointSet" type="rsmCommon:DiscretisedPointSet">
            <xs:annotation>
              <xs:documentation>A series of points, i.e. an polygon, representing an alignment.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="usesElevationAndInclination" type="rsmCommon:ElevationAndInclination"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="usesHorizontalAlignmentSegment" type="rsmCommon:HorizontalAlignmentSegment"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="usesIntrinsicCoordinate" type="rsmCommon:IntrinsicCoordinate"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="usesLineAlignmentCurve" type="rsmCommon:AlignmentCurve">
            <xs:annotation>
              <xs:documentation>A alignment curve that represents the chainage line.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="usesPositioningSystem" type="rsmCommon:PositioningSystem"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="usesPositioningSystemCoordinate" type="rsmCommon:PositioningSystemCoordinate">
            <xs:annotation>
              <xs:documentation>Coordinates in the national mapping grid.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="usesVerticalAlignmentSegment" type="rsmCommon:VerticalAlignmentSegment"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="MaterialTypes">
    <xs:annotation>
      <xs:documentation>A list of material types.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="timber">
        <xs:annotation>
          <xs:documentation>wood</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="concrete">
        <xs:annotation>
          <xs:documentation>beton</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="steel">
        <xs:annotation>
          <xs:documentation>steel sleepers are incompatible with track circuits</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="synthetic">
        <xs:annotation>
          <xs:documentation>plastic
DE: Kunststoff
NL: kunststof
FR: mati&amp;#232;re plastique </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="other">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType abstract="true" name="BaseObject">
    <xs:annotation>
      <xs:documentation>The EULYNX base object inherits an identity from the RSM base object. This identity acts as an unique anchor that other objects can refer to. </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="rsmCommon:BaseObject"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="tElementWithIDref">
    <xs:attribute name="ref" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation>The XSD generator can be hard coded so that the ref attribute is restricted to the pattern (urn:uuid:)?[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}|\{[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}\}</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:element name="dataContainer" type="generic:DataContainer"/>
  <xs:complexType name="DataContainer">
    <xs:annotation>
      <xs:documentation>Container for a structured data set that describes the state of signalling assets in a data release.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:BaseObject">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="hasExplanatoryNote" type="generic:Attachment">
            <xs:annotation>
              <xs:documentation>Details how the data release was produced. This allows a user of a snapshot data set to assess the quality of the data. E.g. notes about conversion problems or gaps in the data.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="ownsDataPrepEntities" type="generic:DataPrepEntities">
            <xs:annotation>
              <xs:documentation>Storage for EULYNX DP objects. Objects that are in the national domain, e.g. NR, ProRail, DB, SNCF, etc., are stored in their own instance of signalling entities.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="ownsRsmEntities" type="generic:RsmEntities"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="LineSideDetectionEvaluator">
    <xs:annotation>
      <xs:documentation>A line-side detection evaluator typically processes inputs from detectors of dangerous situations. </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:LineSideSpotSystem">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="1" name="isOfDetectorType" nillable="true" type="generic:LineSideDetectorTypes"/>
          <xs:element maxOccurs="1" minOccurs="0" name="reportsAlarms" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>True when the evaluator reports alarms. False otherwise.
Not informed when irrelevant.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="reportsAlarmsTo" type="xs:string">
            <xs:annotation>
              <xs:documentation>Describes the system to which alarms are reported.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType abstract="true" name="ObjectStatus">
    <xs:annotation>
      <xs:documentation>An object-configuration can reflect a particular status, typically in construction phases.
For instance, a signal can be disconnected so drivers disregard the signal. The signal would then own a configuration associated with an out-of-service status.
This abstract class reflects an object status and the IM can specialise the object status that describes national needs.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:ConfigurationProperty"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="ProjectMetaData">
    <xs:annotation>
      <xs:documentation>Organisational and subject meta-data that apply globally to planning groups within a project stage.
de-DE: LST Planung Projekt</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:BaseObject">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="hasDateOfCompletion" type="xs:dateTime">
            <xs:annotation>
              <xs:documentation>The planned finish date of the project. 
This allows sorting projects by date.
Usually, only the year, month and optionally day are meaningful.
</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="hasProjectLead" type="generic:Actor">
            <xs:annotation>
              <xs:documentation>The responsible project manager (PM). May be filled with minimal data if there are concerns about privacy.
fr-FR: SNCF: ce champ est obligatoire.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="isOwnedByOrganisation" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Designates the organisational unit with the Infrastructure Manager (IM) in charge of the project.
E.g. IM regional organisation or corridor project organisation</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>generic:Organisation</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="knowsOrganisation" type="generic:Organisation">
            <xs:annotation>
              <xs:documentation>Information about stakeholder organisations are stored in the project meta data.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="programmeDesignation" type="xs:string">
            <xs:annotation>
              <xs:documentation>Top level scheme or programme designation.
E.g. national ETCS roll-out.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="1" name="projectDesignation" nillable="true" type="xs:string">
            <xs:annotation>
              <xs:documentation>The designation of the project.
E.g.  ETCS resignalling yard Euville-la-Jolie.
en-UK: project or scheme</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="projectNumber" type="xs:string">
            <xs:annotation>
              <xs:documentation>The project number used for accounting purposes. Represented by a string so it can be composed of several numbers.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="1" name="splitsIntoGroupMetaData" nillable="true" type="generic:ProjectStageGroupMetaData">
            <xs:annotation>
              <xs:documentation>The project meta-data splits into one or more groups of meta-data. </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="ProjectStageGroupMetaData">
    <xs:annotation>
      <xs:documentation>Meta-data informing a group of project stages. Stages are grouped to reflect common qualities such as:
&lt;ul&gt;
 &lt;li&gt;allocation to consulting engineering firm (e.g. stages x-y by ACME railway engineering GmbH).&lt;/li&gt;
&lt;/ul&gt;
&lt;ul&gt;
 &lt;li&gt;area&lt;/li&gt;
 &lt;li&gt;discipline (aka subject matter), e.g. geo, interlocking, ETCS&lt;/li&gt;
&lt;/ul&gt;</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:BaseObject">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="coversObservationArea" type="rsmCommon:AreaLocation">
            <xs:annotation>
              <xs:documentation>This group of planning stages covers the given observation area. The objects in this area cannot be changed.
de-DE: Betrachtungsbereich</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="coversPlanningArea" type="rsmCommon:AreaLocation">
            <xs:annotation>
              <xs:documentation>Area where the actual engineering is carried out. Changes to objects are allowed and to be expected within this area.
de-DE: Planungsbereich</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="dataCategory" type="xs:string">
            <xs:annotation>
              <xs:documentation>Marks the stage in the data-flow life cycle. E.g. 
&lt;ul&gt;
 &lt;li&gt;first digitisation of as-is quality checked data (first assembly of digital inventory).&lt;/li&gt;
 &lt;li&gt;Insertion of topological nodes to divide-and-rule&lt;/li&gt;
 &lt;li&gt;Version bumping&lt;/li&gt;
&lt;/ul&gt;
Similar to &lt;i&gt;planningType&lt;/i&gt; attribute in project stage meta data. 
SNCF: not stored in plans because done in the data factory.
DB: Planung_G_Art_Besonders

</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="discipline" type="xs:string">
            <xs:annotation>
              <xs:documentation>Discipline, subject matter, e.g. interlocking, geo-information, ETCS, etc.
This supports the compartmentalising and sequencing signalling design, typically
1. GEO layout
2. IL engineering
3. signal positioning and sighting
4. ETCS engineering
A responsible designer can sign for the compartment of the engineering work under his responsibility, Mr. S. Signaller signed off for his discipline, IL engineering, only. </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="hasDateOfCompletion" type="xs:dateTime">
            <xs:annotation>
              <xs:documentation>The planned date of completion of this group of project stages.
Supports project managers to forecast, timely ordering, and resource planning.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="hasReferenceOperationalLocality" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Reference to the locality to which the meta-data apply. This links the project to company-wide planning and documentation. E.g. Projet Grand Paris&lt;i&gt;.&lt;/i&gt;</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>generic:OperationalLocality</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="hasRequirementSpecification" type="generic:Attachment">
            <xs:annotation>
              <xs:documentation>Document describing the scheme requirements, operational objectives.
e.g.  "increase speed on Dresden - Leipzig to 200 kmh" 
This helps the supplier to design requirement validation tests.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="isCreatedByOrganisation" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The design organisation, engineering company, that created the plans for this group of plans.
fr-FR: bureau d'&amp;#233;tudes (champ obligatoire)
de-DE: Ingenieurb&amp;#252;ro</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>generic:Organisation</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="isOwnedByOrganisation" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The sub-organisation of the IM or regional organisation of the IM that is responsible for, and owns the plans and data.

This allows addressing the correct organisation.

SNCF: signifie la r&amp;#233;gion en &lt;i&gt;possession des plans &lt;/i&gt;(champ obligatoire)
</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>generic:Organisation</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="modifiedSubSystem" type="xs:string">
            <xs:annotation>
              <xs:documentation>Designates the subsystem that is subject to planned changes, e.g. decentral interlocking.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="modifiedSystem" type="xs:string">
            <xs:annotation>
              <xs:documentation>Designates the facility or system that is subject to planned changes, e.g. "signalling system" or  "interlocking central unit".</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="projectSectionDescription" type="xs:string">
            <xs:annotation>
              <xs:documentation>Description of the geographic zone under planning. 
Typically indicates that the present group of project stages applies to a section of a larger project
E.g "section Dresden Hbf - Dresden Neustadt" (which is part of the larger project Dresden - Leipzig)

DE: Bauabschnitt</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="referenceLineKilometre" type="xs:string">
            <xs:annotation>
              <xs:documentation>Reference chainage (aka kilometrage/mileage), where this group of project stages is centered, or e.g. the largest station in the planning area.
E.g. km 125,35 +5,25 (m)
en-UK: Engineer's line reference (ELR)
de-DE: Strecke-KM auf fuehrender Strecke
Note that the planning area location provides the precise machine-readable information.

</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="referenceLineSection" type="xs:string">
            <xs:annotation>
              <xs:documentation>Informs the limits on the reference line of the planning area, e.g. "Km 12.7 to Km 13.5".
Exact machine-readable information is found in the planning area.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="refersToProjectReferenceLine" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>This group of project stages is centered on this line.
de-DE: fuehrende Strecke
en-UK: Engineer's Line Reference.
Definition of the leading line supports orienting and plotting layout plans according to rules such as "the project plan shall be drawn such that the leading line plots with kilometrage counting up from left to right" or "London to the left", or "Paris &amp;#224; gauche, kilom&amp;#232;tre 0".</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>generic:Line</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="requestedSchemaVersion" type="xs:string">
            <xs:annotation>
              <xs:documentation>Version of the EULYNX DP schema (XSD version) that the data engineer was requested to use for the present data set.
Note that a validation tool looks for xsi:schemaLocation in the header of the XML instance for the XSD to validate against. This XSD can differ from the present attribute because data engineers may need to apply a different version in the course of their work. </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="1" name="splitsIntoStageMetaData" nillable="true" type="generic:ProjectStageMetaData">
            <xs:annotation>
              <xs:documentation>The property group splits into meta-data for this single stage.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="stageGroupDesignation" type="xs:string">
            <xs:annotation>
              <xs:documentation>Designation of the group of planning stages, expressing the intent of the grouping, e.g.  "stages resignalling design by ACME signalisation ferroviaire S.A."</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="LineSideLinearConstruction">
    <xs:annotation>
      <xs:documentation>Facade for linear- or area-located constructions such as over bridge, under bridge, tunnel, noise protection barrier.
The construction has either a linear- or an area-location.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:LineSideInstallation">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="isAreaLocatedAt" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The line-side construction has an area location. Appropriate for dual-track tunnels or bridges over several tracks.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>rsmCommon:AreaLocation</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="isLinearLocatedAt" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The line-side construction has a linear location. Appropriate for constructions like a noise wall or a single-track tunnel.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>rsmCommon:LinearLocation</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="dataPrepEntities" type="generic:DataPrepEntities"/>
  <xs:complexType name="DataPrepEntities">
    <xs:annotation>
      <xs:documentation>Container for objects in the EULYNX DP signalling domain.
de-DE: LST Daten</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:BaseObject">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="knowsAssetAndState" type="sig:AssetAndState"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsAccessPoint" type="sig:AccessPoint"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsAlignmentCantSegment" type="generic:AlignmentCantSegment"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsAnchorPoint" type="generic:AnchorPoint"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsAnnotation" type="generic:Annotation"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsAspect" type="sig:Aspect"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsAspectProving" type="sig:AspectProving"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsAssetUser" type="generic:AssetUser"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsAtpProtectiveDistance" type="sig:TpsProtectionDistance"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsAttachmentDocument" type="generic:Attachment"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsAutomatic" type="sig:Automatic">
            <xs:annotation>
              <xs:documentation>Set of autoroutes</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsAutomaticRouteSettingSystem" type="sig:AutomaticRouteSettingSystem"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsBackToBackLocking" type="sig:BackToBackLocking"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsBaliseGroupLinking" type="sig:BaliseGroupLinking"/>
