<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:db="http://dataprep.eulynx.eu/schema/DB/1.0" xmlns:generic="http://dataprep.eulynx.eu/schema/Generic/1.0" xmlns:nr="http://dataprep.eulynx.eu/schema/NR/1.0" xmlns:prorail="http://dataprep.eulynx.eu/schema/ProRail/1.0" xmlns:rfi="http://dataprep.eulynx.eu/schema/RFI/1.0" xmlns:rsmCommon="http://www.railsystemmodel.org/schemas/Common/1.2" xmlns:rsmSig="http://www.railsystemmodel.org/schemas/Signalling/1.2" xmlns:rsmTrack="http://www.railsystemmodel.org/schemas/Track/1.2" xmlns:sig="http://dataprep.eulynx.eu/schema/Signalling/1.0" xmlns:sncf="http://dataprep.eulynx.eu/schema/SNCF/1.0" xmlns:trv="http://dataprep.eulynx.eu/schema/TRV/1.0" elementFormDefault="qualified" targetNamespace="http://dataprep.eulynx.eu/schema/Generic/1.0">
  <!--<dc:date>2021-12-21T16:28:00Z</dc:date><dc:creator>rtm2xsd 1.9.1</dc:creator><dc:source>Model 1.0, XMI 2.4.1, EA 13</dc:source>-->
  <xs:annotation>
    <xs:documentation>
