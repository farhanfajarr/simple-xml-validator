  <xs:complexType name="KvbSpeedControlInformationField">
    <xs:annotation>
      <xs:documentation>Information linked to speed control.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sncf:KvbInformationField">
        <xs:sequence>
          <xs:element name="hasKvbSpeedControlInformationType" nillable="true" type="sncf:KvbSpeedControlInformationTypes"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="hasSpeedDesignProperties" type="sncf:KvbSpeedDesignProperties"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="hasTarget" type="sncf:KvbTpsToTargetRelation"/>
        </xs:sequence>
