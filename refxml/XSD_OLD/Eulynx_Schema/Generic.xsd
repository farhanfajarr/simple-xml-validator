<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:generic="http://dataprep.eulynx.eu/schema/Generic" xmlns:rsmCommon="http://www.railsystemmodel.org/schemas/RSM1.2beta/Common" xmlns:rsmSig="http://www.railsystemmodel.org/schemas/RSM1.2beta/Signalling" xmlns:rsmTrack="http://www.railsystemmodel.org/schemas/RSM1.2beta/Track" xmlns:sig="http://dataprep.eulynx.eu/schema/Signalling" elementFormDefault="qualified" targetNamespace="http://dataprep.eulynx.eu/schema/Generic">
  <!--<dc:date>2021-08-31T12:05:18Z</dc:date><dc:creator>rtm2xsd 1.8.5</dc:creator><dc:source>Model , XMI 2.4.1, EA 13</dc:source>-->
  <xs:import namespace="http://dataprep.eulynx.eu/schema/Signalling" schemaLocation="Signalling.xsd"/>
  <xs:import namespace="http://www.railsystemmodel.org/schemas/RSM1.2beta/Track" schemaLocation="../RSM_Schema/Track.xsd"/>
  <xs:import namespace="http://www.railsystemmodel.org/schemas/RSM1.2beta/Signalling" schemaLocation="../RSM_Schema/Signalling.xsd"/>
  <xs:import namespace="http://www.railsystemmodel.org/schemas/RSM1.2beta/Common" schemaLocation="../RSM_Schema/Common.xsd"/>
  <xs:complexType name="Topography">
    <xs:annotation>
      <xs:documentation>Container for topography, i.e. position and alignment components. 
Coordinates are typically geographic but may include paper and display coordinates.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:BaseObject">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="usesAlignmentCantSegment" type="rsmCommon:AlignmentCantSegment"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="usesAlignmentCurve" type="rsmCommon:AlignmentCurve"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="usesHorizontalAlignmentSegment" type="rsmCommon:HorizontalAlignmentSegment"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="usesLineAlignmentCurve" type="rsmCommon:AlignmentCurve"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="usesPositioningSystem" type="rsmCommon:PositioningSystem"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="usesPositioningSystemCoordinate" type="rsmCommon:PositioningSystemCoordinate"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="usesVerticalAlignmentSegment" type="rsmCommon:VerticalAlignmentSegment"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="MaterialTypes">
    <xs:annotation>
      <xs:documentation>A list of material types.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="timber">
        <xs:annotation>
          <xs:documentation>wood</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="concrete">
        <xs:annotation>
          <xs:documentation>beton</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="steel">
        <xs:annotation>
          <xs:documentation>steel sleepers are incompatible with track circuits</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="synthetic">
        <xs:annotation>
          <xs:documentation>plastic
DE: Kunststoff
NL: kunststof
FR: mati&amp;#232;re plastique </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="other">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType abstract="true" name="BaseObject">
    <xs:annotation>
      <xs:documentation>The EULYNX base object inherits an identity from the RSM base object. This identity acts as an unique anchor that other objects can refer to. </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="rsmCommon:BaseObject"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="tElementWithIDref">
    <xs:attribute name="ref" type="xs:string"> <!--use="required"-->
      <xs:annotation>
        <xs:documentation>The XSD generator can be hard coded so that the ref attribute is restricted to the pattern (urn:uuid:)?[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}|\{[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}\}</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:element name="dataContainer" type="generic:DataContainer"/>
  <xs:complexType name="DataContainer">
    <xs:annotation>
      <xs:documentation>Container for structured data relevant to signalling.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:BaseObject">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="hasExplanatoryNote" type="generic:Attachment">
            <xs:annotation>
              <xs:documentation>Details how the data release was produced that allow a user of a snapshot data set to assess the quality of the data. E.g. notes about conversion problems or gaps in the data, </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="ownsRsmEntities" type="generic:RsmEntities"/>
          <xs:element maxOccurs="1" minOccurs="0" name="ownsSignallingEntities" type="generic:SignallingEntities"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="LineSideDetector">
    <xs:annotation>
      <xs:documentation>Line-side detector, typically used to detect dangerous situations. </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:LineSideSpotDevice">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="1" name="isOfType" nillable="true" type="generic:LineSideDetectorTypes"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType abstract="true" name="ObjectStatus">
    <xs:annotation>
      <xs:documentation>An object-configuration can reflect a particular status, typically in construction phases.
For instance, a signal can be disconnected so drivers disregard the signal. The signal would then own a configuration associated with an out-of-service status.
This abstract class reflects an object status and the IM can specialise the object status that describes national needs.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:ConfigurationProperty"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="ProjectMetaData">
    <xs:annotation>
      <xs:documentation>Organisational and subject meta-data that apply to planning groups within a project stage.
de-DE: LST Planung Projekt</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:BaseObject">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="1" name="IMorganisation" nillable="true" type="xs:string">
            <xs:annotation>
              <xs:documentation>Designates the organisational unit with the Infrastructure Manager (IM) in charge of the project. </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="finishByDate" nillable="true" type="xs:dateTime">
            <xs:annotation>
              <xs:documentation>The planned finish date of the project. 
This allows sorting projects by date.
Usually, only the year, month and optionally day are meaningful.
</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="hasProjectLead" nillable="true" type="generic:Actor">
            <xs:annotation>
              <xs:documentation>The responsible project manager (PM)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="knowsOrganisation" type="generic:Organisation">
            <xs:annotation>
              <xs:documentation>Information about stakeholder organisations are stored in the project meta data.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="programmeDesignation" type="xs:string">
            <xs:annotation>
              <xs:documentation>Top level scheme or programme designation, e.g. "national ETCS roll-out"</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="1" name="projectDesignation" nillable="true" type="xs:string">
            <xs:annotation>
              <xs:documentation>The designation of the project, e.g.  "ETCS resignalling yard Euville-la-Jolie" 
en-UK: project or scheme</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="1" name="projectNumber" nillable="true" type="xs:string">
            <xs:annotation>
              <xs:documentation>The project number used for accounting purposes.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="1" name="splitsIntoGroupMetaData" nillable="true" type="generic:ProjectStageGroupMetaData"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="HeightInformation">
    <xs:annotation>
      <xs:documentation>Spot-located height over a given reference level. </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:VerticalInformation">
        <xs:sequence>
          <xs:element name="usesDatum" nillable="true" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Defines the reference level for the height</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>rsmCommon:GeographicCoordinateSystem</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="ProjectStageGroupMetaData">
    <xs:annotation>
      <xs:documentation>Meta-data that apply to a group of planning stages. Stages are grouped to reflect common qualities such as
&lt;ul&gt;
 &lt;li&gt;allocation to consulting engineering firm (e.g. stages x-y by ACME railway consulting GmbH).&lt;/li&gt;
&lt;/ul&gt;
&lt;ul&gt;
 &lt;li&gt;area&lt;/li&gt;
 &lt;li&gt;discipline (aka subject matter), e.g. geo, interlocking, ETCS&lt;/li&gt;
&lt;/ul&gt;

Similar to planningType attribute in project stage meta data. </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:BaseObject">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="coversObservationArea" type="rsmCommon:AreaLocation">
            <xs:annotation>
              <xs:documentation>This group of planning stages covers the given observation area. The objects in this area may not be changed.
de-DE: Betrachtungsbereich</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="coversPlanningArea" type="rsmCommon:AreaLocation">
            <xs:annotation>
              <xs:documentation>Area where the actual signalling engineering is carried out. Changes to objects are allowed and to be expected within this area.
de-DE: Planungsbereich</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="createdByOrganisation" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The design organisation ("engineering bureau") that created the plans for this group of plans.
fr-FR: champ obligatoire</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>generic:Organisation</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="dataCategory" type="xs:string">
            <xs:annotation>
              <xs:documentation>Marks the stage in the data-flow life cycle. E.g. 
&lt;ul&gt;
 &lt;li&gt;first digitisation of as-is quality checked data (first assembly of digital inventory).&lt;/li&gt;
 &lt;li&gt;Insertion of topological nodes to divide-and-rule&lt;/li&gt;
 &lt;li&gt;Version bumping&lt;/li&gt;
&lt;/ul&gt;
SNCF: not stored in plans because done in the data factory

</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="discipline" type="xs:string">
            <xs:annotation>
              <xs:documentation>Discipline, subject matter, e.g. interlocking, geo-information, ETCS, ...
This supports the compartmentalising and sequencing signalling design, typically
1. GEO layout
2. IL engineering
3. signal positioning and sighting
4. ETCS engineering
A responsible designer can sign for the compartment of the engineering work under his responsibility, Mr. S. Signaller signed off for his discipline, IL engineering, only. </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="hasDateOfCompletion" nillable="true" type="xs:dateTime">
            <xs:annotation>
              <xs:documentation>The planned date of completion of this group of project stages.

Supports project managers to forecast, timely ordering, and resource planning.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="hasReferenceOperationalLocality" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Supports allocating the project to company-wide planning, documentation and naming the documents.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>generic:OperationalLocality</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="hasRequirementSpecification" type="generic:Attachment">
            <xs:annotation>
              <xs:documentation>Document describing the scheme requirements, operational objectives.
e.g.  "increase speed on Dresden - Leipzig to 200 kmh" 
This helps the supplier to design requirement validation tests.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="isOwnedByOrganisation" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The sub-organisation of the IM or regional organisation of the IM that is responsible for, and owns the plans and data.

This allows addressing the correct organisation.

SNCF: champs obligatoire: signifie la r&amp;#233;gion en &lt;i&gt;possession des plans.&lt;/i&gt;
</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>generic:Organisation</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="lineSection" type="xs:string">
            <xs:annotation>
              <xs:documentation>Informs the limits on the reference line of the planning area, e.g. "Km 12.7 to Km 13.5".
Exact machine-readable information is found in the planning area.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="modifiedSubSystem" type="xs:string">
            <xs:annotation>
              <xs:documentation>Designates the subsystem that is subject to planned changes, e.g. decentral interlocking.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="modifiedSystem" type="xs:string">
            <xs:annotation>
              <xs:documentation>Designates the facility or system that is subject to planned changes, e.g. "signalling system" or  "interlocking central unit".</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="projectSectionDescription" type="xs:string">
            <xs:annotation>
              <xs:documentation>Description of the geographic zone under planning. 
Typically indicates that the present group of project stages applies to a section of a larger project
E.g "section Dresden Hbf - Dresden Neustadt" (which is part of the larger project Dresden - Leipzig)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="referenceLineKilometre" type="xs:string">
            <xs:annotation>
              <xs:documentation>Reference chainage (kilometrage/mileage), where this group of project stages is centered, or e.g. the largest station in the planning area.
E.g. km 125,35 +5,25 (m)
en-UK: Engineer's line reference (ELR)
de-DE: Strecke-KM auf fuehrender Strecke
Note that the planning area location provides the precise machine readable information.

</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="refersToProjectReferenceLine" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>This group of project stages is centered on this line.
de-DE: fuehrende Strecke
en-UK: Engineer's Line Reference.
Definition of the leading line supports orienting and plotting layout plans according to rules such as "the project plan shall be drawn such that the leading line plots with kilometrage counting up from left to right" Or  "London to the left", or "Paris &amp;#224; gauche, kilom&amp;#232;tre 0".</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>generic:Line</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="requestedSchemaVersion" type="xs:string">
            <xs:annotation>
              <xs:documentation>Version of the EULYNX DP schema (XSD version) that the data engineer was requested to use when filling a data set.
A validation tool would look in the header of the XML instance for the XSD to validate against. 
The header could theoretically differ from this attribute e.g. because data engineers are lead in the course of their work to apply a different version. </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="1" name="splitsIntoStageMetaData" nillable="true" type="generic:ProjectStageMetaData">
            <xs:annotation>
              <xs:documentation>The property group splits into meta-data for this single stage</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="stageGroupDesignation" type="xs:string">
            <xs:annotation>
              <xs:documentation>Designation of the group of planning stages, expressing the intent of the grouping, e.g.  "stages resignalling design by ACME signalisation ferroviaire S.A."</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="LineSideLinearConstruction">
    <xs:annotation>
      <xs:documentation>Facade for linear located constructions like
&lt;ul&gt;
 &lt;li&gt;Bridge&lt;/li&gt;
 &lt;li&gt;Tunnel&lt;/li&gt;
 &lt;li&gt;Noise protection barrier&lt;/li&gt;
&lt;/ul&gt;</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:LineSideInstallation">
        <xs:sequence>
          <xs:element name="isLocatedAt" nillable="true" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>rsmCommon:LinearLocation</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="signallingEntities" type="generic:SignallingEntities"/>
  <xs:complexType name="SignallingEntities">
    <xs:annotation>
      <xs:documentation>Container for objects in the EULYNX DP signalling domain.
de-DE: LST Daten</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:BaseObject">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="knowsAssetAndState" type="sig:AssetAndState"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsAnnotation" type="generic:Annotation"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsAspect" type="sig:Aspect"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsAspectProving" type="sig:AspectProving"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsAssetUser" type="generic:AssetUser"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsAtpProtectiveDistance" type="sig:TpsProtectionDistance"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsAttachmentDocument" type="generic:Attachment"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsAutomatic" type="sig:Automatic">
            <xs:annotation>
              <xs:documentation>Set of autoroutes</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsBackToBackLocking" type="sig:BackToBackLocking"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsBaliseGroupLinking" type="sig:BaliseGroupLinking"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsBaselineOfRuleSet" type="generic:BaselineOfRuleSet"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsBlockSystem" type="sig:BlockSystem"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsCable" type="sig:Cable"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsCableDuct" type="sig:CableRoute"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsCableJoint" type="sig:CableJoint"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsCableJointInstallation" type="sig:CableJointInstallation"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsCableProperties" type="sig:CableProperties"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsCableRouteJunction" type="sig:CableRouteJunction"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsChangeMarker" type="sig:ChangeMarker"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsCivilStructure" type="sig:CivilStructure"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsCompositeRoute" type="sig:CompositeRoute"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsConditionSectionsClear" type="sig:ConditionSectionsClear"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsConflictingRoute" type="sig:ConflictingRoute"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsControlArea" type="sig:ControlArea"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsControlRelation" type="sig:ControlRelation"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsControlledTrackAsset" type="sig:ControlledTrackAsset"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsDADoverlap" type="sig:DirectionAuthorityDependentOverlap"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsDangerAreaTrack" type="sig:DangerAreaTrack"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsDescription" type="generic:Description"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsDispatchAnnouncerAssignment" type="sig:DispatchAnnouncerAssignment"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsEquipmentSupport" type="sig:EquipmentSupport"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsEtcsAcknowledgement" type="sig:EtcsAcknowledgement"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsEtcsSignalProperties" type="sig:EtcsSignalProperties"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsEtcsSystemVersion" type="sig:EtcsSystemVersion"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsEtcsTelegramConditionRelation" type="sig:EtcsTelegramConditionRelation"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsFlankProtection" type="sig:FlankProtection"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsFoulTrackControl" type="sig:FoulTrackControl"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsIllumination" type="sig:Illumination"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsInfrastructureBorder" type="sig:InfrastructureBorder"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsIntentionallyDarkAllowed" type="sig:IntentionallyDarkAllowed"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsKey" type="sig:Key">
            <xs:annotation>
              <xs:documentation>Physical or numerical keys used to unlock (sub-)systems of the interlocking.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsLensDiffuser" type="sig:LensDiffuser"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsLevelCrossingActivation" type="sig:LevelCrossingActivation"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsLevelCrossingDeactivation" type="sig:LevelCrossingDeactivation"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ownsLevelCrossingLocalOperationUiElement" type="sig:LevelCrossingLocalOperationUiElement"/>
