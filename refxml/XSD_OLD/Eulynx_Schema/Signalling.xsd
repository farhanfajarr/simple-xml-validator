          </xs:element>
          <xs:element name="refersToLocalDatum" nillable="true" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Points to the location of interest/reference/description that is used as the local datum.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>generic:LocationProxy</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
