                <ObjectTypeRef>sig:Controller</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="endPointBTrackAsset" nillable="true" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
