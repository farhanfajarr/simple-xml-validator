  <xs:complexType name="PowerSourceRelation">
    <xs:annotation>
      <xs:documentation>Describes the sourcing of power to a power supply. </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:BaseObject">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="1" name="ProvidesPowerTo" nillable="true" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>sig:PowerSupplyModule</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="getsPowerFromPsModule" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Gets power from a physical power supply module.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>sig:PhysicalPowerSupplyModule</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="getsPowerFromTrackAsset" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The virtual power supply model gets power from a track asset such as a controller, LX, or TPS device.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>sig:TrackAsset</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="hasPowerSourceType" type="sig:PowerSupplyTypes">
            <xs:annotation>
              <xs:documentation>The power source type is indicated if no actual object that is the source of power can be indicated.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="1" name="isPrimarySource" nillable="true" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>True if the source of the power is the primary power.
False otherwise.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="1" name="isUps" nillable="true" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>True if the source of power is buffered (UPS - Uninterruptable Power Supply)
de-DE: USV Unterbrechungsfreie Stromversorgung.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="SidingTrack">
    <xs:annotation>
      <xs:documentation>Track not considered part of the main infrastructure
de-DE: Anschlussgleis, Werksgleis
en-GB: used for shunting
nl-NL: raccordement, opstelspoor
fr-FR: voie de service</xs:documentation>
    </xs:annotation>
