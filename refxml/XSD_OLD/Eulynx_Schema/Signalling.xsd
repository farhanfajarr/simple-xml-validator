      <xs:documentation>The electric properties of a LEU cabinet.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:ElectricProperties">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="1" name="numberOfLeuSystems" nillable="true" type="xs:int">
