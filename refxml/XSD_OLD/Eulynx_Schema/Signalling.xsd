      <xs:documentation>Marks a &lt;i&gt;spot location&lt;/i&gt; from where a speed applies.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:EtcsMarker">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="hasCantDeficiencySpeed" type="sig:CantDeficiencySpeed"/>
          <xs:element maxOccurs="1" minOccurs="0" name="hasChangeCondition" type="sig:EtcsChangeCondition">
            <xs:annotation>
              <xs:documentation>The speed change marker applies under the given condition.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="hasEtcsTrainCategorySpeed" type="sig:EtcsTrainCategorySpeed">
            <xs:annotation>
              <xs:documentation>Has speed depending on ETCS train category.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="1" name="q_diff" nillable="true" type="xs:int"/>
          <xs:element maxOccurs="1" minOccurs="1" name="q_front" nillable="true" type="xs:boolean"/>
          <xs:element maxOccurs="1" minOccurs="1" name="v_static" nillable="true" type="xs:int">
            <xs:annotation>
              <xs:documentation>Maps to V_STATIC, basic SSP.</xs:documentation>
            </xs:annotation>
          </xs:element>
