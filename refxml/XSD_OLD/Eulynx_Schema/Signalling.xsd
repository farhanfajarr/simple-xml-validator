  <xs:complexType name="CableProperties">
    <xs:complexContent>
      <xs:extension base="generic:BaseObject">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="1" name="appliesToCable" nillable="true" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>sig:Cable</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="capacitiveCoupling" type="xs:int">
            <xs:annotation>
              <xs:documentation>In picoFarad per kilometre.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="1" name="category" nillable="true" type="xs:string">
