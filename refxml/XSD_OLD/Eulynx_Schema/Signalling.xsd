                <ObjectTypeRef>sig:EtcsTelegram</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="toBaliseGroup" nillable="true" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
