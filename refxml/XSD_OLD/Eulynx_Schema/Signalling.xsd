  <xs:complexType name="EtcsPointCondition">
    <xs:annotation>
      <xs:documentation>The change is conditional to the given points being in the given position.
When no point condition is given, the ETCS marker doesn't depend on the path.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:EtcsChangeCondition">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="1" name="appliesWhenPointElementInPosition" nillable="true" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The marker action applies only when the point element(s) are in the given position.
de-DE: Entscheidungsweiche</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>sig:PointElementAndPosition</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="RoadTrafficSign">
    <xs:annotation>
      <xs:documentation>Road traffic light or sign. Assembly drawings can be attached or referred to.
en-UK: Road Traffic Light (RTL)</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="appliesToLevelCrossingSystem" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The road traffic sign is associated with this level crossing system</xs:documentation>
