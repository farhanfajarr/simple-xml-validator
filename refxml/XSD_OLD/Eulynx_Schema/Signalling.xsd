              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="hasDistance" nillable="true" type="rsmCommon:Length">
            <xs:annotation>
              <xs:documentation>Distance between from-to balise groups (N_PIG=0)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="hasLinkReaction" type="sig:LinkReactionTypes">
            <xs:annotation>
              <xs:documentation>The link reaction, given when it differs from the standard reaction as defined by rules.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="hasTargetOrientation" nillable="true" type="sig:Orientation">
            <xs:annotation>
              <xs:documentation>The target balise group has the given orientation with respect to the from-to direction.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="1" name="refersToTelegram" nillable="true" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
