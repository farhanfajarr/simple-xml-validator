              <xs:appinfo>
                <ObjectTypeRef>sig:LocalOperatingZone</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="appliesToLocalOperationUiElement" nillable="true" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>sig:LocalOperationUiElement</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="hasFunctionType" nillable="true" type="sig:UiElementFunctionTypes">
