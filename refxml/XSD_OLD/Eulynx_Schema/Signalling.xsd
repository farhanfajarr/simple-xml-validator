              <xs:documentation>The subversion of Subset-026-7.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="1" name="srsVersion" nillable="true" type="xs:string">
            <xs:annotation>
              <xs:documentation>The version of Subset-026-7.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
