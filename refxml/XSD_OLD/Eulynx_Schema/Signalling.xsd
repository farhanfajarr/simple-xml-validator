    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:EtcsAcknowledgement">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="1" name="l_ackmamode" nillable="true" type="xs:float"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType abstract="true" name="RouteExtremity">
    <xs:annotation>
      <xs:documentation>An abstraction of either a route entry or a route exit.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:BaseObject">
        <xs:sequence>
          <xs:element name="isAssociatedWithSignal" nillable="true" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>sig:Signal</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="TcFeederReceiverTypes">
    <xs:annotation>
      <xs:documentation>The different types of track circuit feeders and receivers.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="receiver">
        <xs:annotation>
          <xs:documentation>UK: letter R
FR: letters Re(cepteur)</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="transmitter">
        <xs:annotation>
          <xs:documentation>UK: letter F(eed)
</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="receiverWithTuner">
        <xs:annotation>
          <xs:documentation>audio frequency receiver
UK: letters RX
FR: Re(cepteur)</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="transmitterWithTuner">
        <xs:annotation>
          <xs:documentation>audio frequency transmitter 
FR: Em(etteur)
UK: TX</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="receiverWithLoop">
        <xs:annotation>
          <xs:documentation>A track side receiver attached to a loop in the track.
FR: reception sur boucle</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ResetStrategyTypes">
    <xs:annotation>
      <xs:documentation>The types of reset strategies.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="fc_c">
        <xs:annotation>
          <xs:documentation>FC-C, Force section status to clear, conditional</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="fc_p">
        <xs:annotation>
          <xs:documentation>FC-P, Force section status to clear, preparatory</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="fc_p_a">
        <xs:annotation>
          <xs:documentation>FC-P-A, Force section status to clear, preparatory, with acknowledgement</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="fc_u">
        <xs:annotation>
          <xs:documentation>FC-U, Force section status to clear, unconditional</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType abstract="true" name="SynchedFlashing">
