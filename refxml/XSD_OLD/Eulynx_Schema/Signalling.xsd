              <xs:documentation>Cross-sectional area in square millimetres.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="diameter" type="xs:float">
            <xs:annotation>
              <xs:documentation>Diameter in mm.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="hasCableCapacity" type="sig:CableCapacity">
            <xs:annotation>
              <xs:documentation>Total capacity of the cable in terms of number of pairs or quads.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="hasSpareCapacity" type="sig:CableCapacity"/>
          <xs:element maxOccurs="1" minOccurs="0" name="modeltype" type="xs:string">
            <xs:annotation>
              <xs:documentation>OEM designation of the cable type.
</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="numberOfCores" type="xs:int">
            <xs:annotation>
              <xs:documentation>Number of cores in the cable.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="operatingVoltage" type="xs:int">
            <xs:annotation>
              <xs:documentation>Maximum sustainable operational voltage.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="ratingPerCore" type="xs:int">
            <xs:annotation>
              <xs:documentation>Rating per core of the cable in Amps.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="resistancePerCore" type="xs:int">
            <xs:annotation>
              <xs:documentation>Resistance per metre per core of the cable in Ohms.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="strandedElements" type="xs:int">
