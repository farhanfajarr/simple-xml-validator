            <xs:annotation>
              <xs:documentation>The containment is made of this material.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="isMountedOnSide" type="sig:DerailmentContainmentLocationTypes">
            <xs:annotation>
              <xs:documentation>The containment is located to this side.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="WarningComponent">
    <xs:annotation>
      <xs:documentation>Audible and/or visual alarm device.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="1" name="isDoubleSided" nillable="true" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>Direction of the alarm.
&lt;ul&gt;
 &lt;li&gt;True: the alarm is omnidirectional.&lt;/li&gt;
 &lt;li&gt;False: the alarm is directed.&lt;/li&gt;
&lt;/ul&gt;</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="isLocatedAt" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>rsmCommon:SpotLocation</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
