<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:generic="http://dataprep.eulynx.eu/schema/Generic" xmlns:rsmCommon="http://www.railsystemmodel.org/schemas/RSM1.2beta/Common" xmlns:rsmSig="http://www.railsystemmodel.org/schemas/RSM1.2beta/Signalling" xmlns:sig="http://dataprep.eulynx.eu/schema/Signalling" elementFormDefault="qualified" targetNamespace="http://dataprep.eulynx.eu/schema/Signalling">
  <!--<dc:date>2021-08-31T12:05:18Z</dc:date><dc:creator>rtm2xsd 1.8.5</dc:creator><dc:source>Model , XMI 2.4.1, EA 13</dc:source>-->
  <xs:import namespace="http://dataprep.eulynx.eu/schema/Generic" schemaLocation="Generic.xsd"/>
  <xs:import namespace="http://www.railsystemmodel.org/schemas/RSM1.2beta/Signalling" schemaLocation="Signalling.xsd"/>
  <xs:import namespace="http://www.railsystemmodel.org/schemas/RSM1.2beta/Common" schemaLocation="Common.xsd"/>
  <xs:simpleType name="ProjectPhaseTypes">
    <xs:restriction base="xs:string">
      <xs:enumeration value="toBuild">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="asBuilt">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="TiltingArea">
    <xs:annotation>
      <xs:documentation>Area equipped with a train protection system for tilting trains.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:TpArea"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="UnderTrackCrossing">
    <xs:annotation>
      <xs:documentation>A cable duct for guiding cables crossing underneath the track. Mostly a pipe.
UTX joins two cable pits, as per the cable duct topology.
&lt;ul&gt;
 &lt;li&gt;en-UK: UTX&lt;/li&gt;
&lt;/ul&gt;
&lt;ul&gt;
 &lt;li&gt;de-DE: Kabelrohr&lt;/li&gt;
&lt;/ul&gt;</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:CableRoute"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="TpwsTrain">
    <xs:annotation>
      <xs:documentation>Category for trains equipped with Train Protection and Warning System.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:TrainCategory">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="1" name="type" nillable="true" type="xs:string">
            <xs:annotation>
              <xs:documentation>Field for the user-defined train category.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="IPversions">
    <xs:restriction base="xs:string">
      <xs:enumeration value="IPv4">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="IPv6">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ClearanceDetectionTypes">
    <xs:restriction base="xs:string">
      <xs:enumeration value="automatic">
        <xs:annotation>
          <xs:documentation>The obstacle detection works fully automatically (typical for radar).</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="semiAutomatic">
        <xs:annotation>
          <xs:documentation>The clearance detection system requires human interaction, i.e. human confirmation that the level crossing is clear. Typical for camera systems. </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="other">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="EtcsPositionReportParametersArea">
    <xs:annotation>
      <xs:documentation>An area where a given set of position report parameters applies.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="isLocatedAt" nillable="true" type="rsmCommon:tElementWithIDref">
        <xs:annotation>
          <xs:appinfo>
            <ObjectTypeRef>rsmCommon:AreaLocation</ObjectTypeRef>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="1" name="t_cycloc" nillable="true" type="xs:int">
        <xs:annotation>
          <xs:documentation>ETCS variable T_CYCLOC </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AspectProving">
    <xs:annotation>
      <xs:documentation>If a signal fails to show a given aspect, then another signal is forced to show another, typically more restrictive, aspect.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:BaseObject">
        <xs:sequence>
          <xs:element name="bestAllowedAspect" nillable="true" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Best allowed signal/message combination when the check fails.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>sig:SignalAndMessage</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="checksAspect" nillable="true" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The aspect proving relation checks this signal/message combination. </xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>sig:SignalAndMessage</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="SystemState">
    <xs:annotation>
      <xs:documentation>A set of (asset,state) pairs that describes the state of the signalling system. </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:BaseObject">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="1" name="definedByAssetAndState" nillable="true" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Reference to a set of (asset, state) tuples.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>sig:AssetAndState</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EtcsOverlap">
    <xs:annotation>
      <xs:documentation>A stretch of track beyond EoA acting as overlap for an approaching ETCS train.
The overlap has a linear location that runs from EoA over a distance D_OL.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:Overlap">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="1" name="automaticReleaseSpeedCalculation" nillable="true" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>True: the release speed is calculated automatically.
False: the allowed train category speed provides the release speed.
</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="hasOverlapTimer" type="rsmCommon:Duration">
            <xs:annotation>
              <xs:documentation>The overlap timer T_OL. If not given, overlap has infinite validity.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="hasReleaseSpeed" type="rsmCommon:Speed">
            <xs:annotation>
              <xs:documentation>maps to V_RELEASEOL</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="hasTimerStartLocation" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The location of the timer start is at a distance of D_STARTOL in rear of the EoA.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>rsmCommon:SpotLocation</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="releaseSpeedDetermination" nillable="true" type="sig:releaseSpeedCalculation">
            <xs:annotation>
              <xs:documentation>Dictates how the train shall determine the release speed.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="CabinetGroup">
    <xs:complexContent>
      <xs:extension base="sig:Cabinet">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="1" name="numberOfCabinets" nillable="true" type="xs:int">
            <xs:annotation>
              <xs:documentation>The number of cabinets (aka cases for NR) that are logically grouped together. Typically close together. 
</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="ControlledTrackAsset">
    <xs:annotation>
      <xs:documentation>Controlled track assets are the subset of track assets that 
&lt;ul&gt;
 &lt;li&gt;are &lt;b&gt;controlled &lt;/b&gt;by, and/or&lt;/li&gt;
 &lt;li&gt;are &lt;b&gt;powered &lt;/b&gt;by, and/or&lt;/li&gt;
 &lt;li&gt;&lt;b&gt;exchange &lt;/b&gt;information with a controller.&lt;/li&gt;
&lt;/ul&gt;
There has to be one instance of this class for each associated track asset instance.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="appliesToActiveSignal" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>sig:ActiveSignal</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="appliesToCompoundLogicalObject" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>this compound logical object is an active track asset</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>sig:CompoundLogicalObject</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="appliesToILIntegratedLevelCrossing" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Informs that the targeted object is a controlled track asset and that there is a control relation between the controlling interlocking and the level crossing system.
The interface associated with the control relation can be EULYNX SCI-LC or SCI-LX.
The level crossing is a controlled track asset and exchanges information/energy with the IL.
</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>sig:IntegratedLevelCrossing</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="appliesToMovableElement" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>This movable element is a controlled track asset.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>sig:MovableElement</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="appliesToReleaseInstrument" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The release instrument is a controlled track asset. In very rare cases, a release instrument doesn't feed back information to the interlocking; the key can only be released when a local condition is satisfied.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>sig:ReleaseInstrument</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="appliesToTpsDevice" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Stand-alone TP devices are not referred to by the controlled track asset class.
A self-powered TP device is not a controlled track asset but if the device has some control relation with a controller, for instance reporting health status, it is a controlled track asset.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>sig:TpsDevice</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="appliesToTrainDetectionSection" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>sig:TdsSection</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="appliesToVehiclePassageDetector" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>this vehicle passage detector is an active track asset</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>sig:VehiclePassageDetector</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="getsBackupPowerFrom" type="sig:PowerSupplyTypes">
            <xs:annotation>
              <xs:documentation>in rare cases, controlled track assets could have a back up power supply</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="isPoweredBy" type="sig:PowerSupplyTypes"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="reportsToDiagnosticsCollector" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Diagnostic data from the controlled track asset are channeled to this diagnostics collector.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>sig:DiagnosticsCollector</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="LockoutArea">
    <xs:annotation>
      <xs:documentation>Area under temporary possession, typically for maintenance activities.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="composedOfLockoutArea" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Grouping of lockout areas representing SNCF "protection".
fr-FR: Groupement de Zones &amp;#233;l&amp;#233;mentaires de protections.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>sig:LockoutArea</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="isLocatedAt" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>rsmCommon:AreaLocation</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="locksOutRoute" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The lock-out area disallows this route.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>sig:Route</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="TpTransitionMarker">
    <xs:annotation>
      <xs:documentation>A spot on the track where a train enters the control area of a another TP system.
de-DE: Bereichsgrenze.
nl-NL: puntafbakening van het beinvloedingsgebied.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:TpMarker"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="PointIndicatorTypes">
    <xs:restriction base="xs:string">
      <xs:enumeration value="backlit">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="reflective">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="SectionList">
    <xs:annotation>
      <xs:documentation>List of sections included in the route body, ordered from entry to exit.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:RouteBodyProperty">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="1" name="hasSection" nillable="true" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>sig:TvpSection</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="LeuRelativePositionTypes">
    <xs:restriction base="xs:string">
      <xs:enumeration value="at">
        <xs:annotation>
          <xs:documentation>The cabinet is placed at or attached to the signal post.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="behind">
        <xs:annotation>
          <xs:documentation>The cabinet is behind the signal, viewed from the signal's application direction.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="inFront">
        <xs:annotation>
          <xs:documentation>The cabinet is in front of the signal, viewed from the signal's application direction.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="SignalAndMessage">
    <xs:annotation>
      <xs:documentation>A pair (signal, message). 
When instantiated, this provides a list of all possible (signal, message) pairs needed for constructing &lt;u&gt;signal plans&lt;/u&gt;. Pairs that are not in the list are disallowed.
</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:AssetAndState">
        <xs:sequence>
          <xs:element name="appliesToSignal" nillable="true" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>sig:Signal</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="showsMessage" nillable="true" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The given signal sends this signal message to the train driver. </xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>sig:SignalMessage</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="LocalOperationDeviceTypes">
    <xs:annotation>
      <xs:documentation>Constructional type of the local operation device.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="panel">
        <xs:annotation>
          <xs:documentation>A vertical panel.
de-DE: Tafel</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="console">
        <xs:annotation>
          <xs:documentation>An inclined panel.
de-DE: Pult
en-UK: part of the UI where the interactive UI elements are. 
</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="column">
        <xs:annotation>
          <xs:documentation>A vertical column with integrated control elements.
de-DE: S&amp;#228;ule</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="tablet">
        <xs:annotation>
          <xs:documentation>A small panel, can be portable.
de-DE: Tablett</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="leverFrame">
        <xs:annotation>
          <xs:documentation>A set of levers, typically used to throw switch blades and (dis-)engage point locks.
en-GB: ground frame</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="other">
        <xs:annotation>
          <xs:documentation>de-DE: sonstige</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="BlockElementConfiguration">
    <xs:annotation>
      <xs:documentation>The configuration of the line block working.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:TrackAssetConfiguration">
        <xs:sequence>
          <xs:element name="appliesToLineBlockElement" nillable="true" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>sig:BlockElement</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="hasBlockNormalisingWarning" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>True if an acoustic warning sounds when the block is normalised.
de-DE: R&amp;#252;ckblockwecker (Pflichtangabe)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="1" name="hasBlockSettingType" nillable="true" type="sig:BlockSettingTypes">
            <xs:annotation>
              <xs:documentation>A block element sets the block in one or more of these ways.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="hasTrainOnLineWarning" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>True if an acoustic warning sounds when a train approaches on the block.
de-DE: Vorblockwecker (Pflichtangabe)</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EquipmentSupport">
    <xs:annotation>
      <xs:documentation>Generalization of fixings and housings, defined as civil structures that can support (or house) signalling equipment. </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element maxOccurs="2" minOccurs="0" name="isCivilStructure" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Reference to a civil structure defined in a different register of preexisting civil assets. E.g. a database of bridges, gantries, posts etc.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>sig:CivilStructure</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="isLocatedAt" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Complex equipment supports may have multiple spot locations, e.g. a gantry with two foundations on each side of the track. The gantry can also support multiple signals, each with its own spot location. 
</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>rsmCommon:SpotLocation</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="supports" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Recursive relation expressing the fact that one type of equipment support can support/carry/house another type of equipment support. For instance: "a foundation supports a control room that houses a cabinet".</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>sig:EquipmentSupport</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="PermanentWarningSystem">
    <xs:annotation>
      <xs:documentation>The warning system is permanently active; staff does not need to activate it.
The warning is triggered by an approaching train.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:WarningSystem">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="warnsWorkersNearStructure" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Refers to a civil structure such as a bridge or tunnel that warrants the presence of the warning system.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>sig:CivilStructure</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="TechnicalCentreHousing">
    <xs:annotation>
      <xs:documentation>Facility that houses computing systems hosting the controller software. This centre is dedicated to running vital signalling applications, as opposed to standard data centres.
de-DE: Technikstandort</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:Building"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="SelfSupervisedLevelCrossing">
    <xs:annotation>
      <xs:documentation>&lt;b&gt;LX&lt;/b&gt; system that has the logic to autonomously processes triggers from route setting and/or train approach. The LX warns and/or closes the barriers when activated. The LX reports failures to the interlocking.
de-DE: F&amp;#252;-B&amp;#220;</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:IntegratedLevelCrossing"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="SignalMessageRelation">
    <xs:annotation>
      <xs:documentation>A signal-message relation is composed of two pairs master(signal,message) and dependent(signal,message).
The message of the master signal dictates the message of the dependent signal.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="appliesToRoute" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The route body to which the signal-message relation applies.
The route body from dependent signal to master signal may contain points.
</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>sig:RouteBody</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="entrySpeed" type="rsmCommon:Speed">
            <xs:annotation>
              <xs:documentation>The speed signalled by the entry signal, aka slave signal. I.e. the speed that the train must respect when passing the entry signal. Also known as passing speed.
This signalled speed can be supervised by an ATP system.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="exitSpeed" type="rsmCommon:Speed">
            <xs:annotation>
              <xs:documentation>Signalled maximum speed at exit signal (aka target or master signal)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="hasDependentMessage" nillable="true" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The aspects of the dependent signal follow the master.
For instance: when the master indicates  "At Danger" the dependent signal indicates "Caution".</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>sig:SignalAndMessage</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="hasEndSectionTimer" type="rsmCommon:Duration">
            <xs:annotation>
              <xs:documentation>The end-section of a route is the section between the closed route exit signal and the previous worker signal. 
The interlocking revokes (part of) the route when this timer expires.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="hasMasterMessage" nillable="true" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The master signal dictates the message that a dependent signal can show.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>sig:SignalAndMessage</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="representsSpeedProfile" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>In speed signalling systems, the aspect relation between signals represents a speed profile that the train must respect. Note that the speed profile is attached to a master and worker signal.
</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>sig:SpeedProfile</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="LightSignal">
    <xs:annotation>
      <xs:documentation>A signal with lamps that transmit information to the driver. </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:ActiveSignal">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="hasLuminosity" type="sig:LuminosityTypes"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="ConditionAffectingSignal">
    <xs:annotation>
      <xs:documentation>Condition required for the signal to show a given aspect, read from a compound logical object or a release instrument.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:RouteCondition">
        <xs:sequence>
          <xs:element name="affectsSignal" nillable="true" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Signal that may open to the given aspect if the conditions in the route relation evaluate to true.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>sig:SignalAndMessage</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="SwitchedWarningSystem">
    <xs:annotation>
      <xs:documentation>The warning system can be switched on when staff is working track side.
The system is triggered by an approaching train.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:WarningSystem"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="TpsSpotDevice">
    <xs:annotation>
      <xs:documentation>A TPS device of limited length.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:TpsDevice"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="DispatchPosition">
    <xs:annotation>
      <xs:documentation>The spot from where trains can be dispatched, i.e. the spot where the dispatcher can sight train(s) and signals. Extra equipment like plungers can be installed here. 
</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element name="isLocatedAt" nillable="true" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>rsmCommon:SpotLocation</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="RouteTractionTypes">
    <xs:restriction base="xs:string">
      <xs:enumeration value="fullyElectrified">
        <xs:annotation>
          <xs:documentation>Suitable for electric traction.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="notFullyElectrified">
        <xs:annotation>
          <xs:documentation>The route (or parts of it) is not electrified. This route is permissible for non-electric vehicles only.
&lt;ul&gt;
 &lt;li&gt;de-DE: F-Bedienung&lt;/li&gt;
&lt;/ul&gt;
&lt;ul&gt;
 &lt;li&gt;nl-NL: Dieseltractie&lt;/li&gt;
&lt;/ul&gt;
&lt;ul&gt;
 &lt;li&gt;fr-FR: Thermique&lt;/li&gt;
&lt;/ul&gt;
Note: this is a static property and does not take into account whether traction power can be switched off.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="CentralInterlocking">
    <xs:annotation>
      <xs:documentation>The part of the interlocking where the core route logic runs. </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:Interlocking"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="RedundancyLayerConfiguration">
    <xs:annotation>
      <xs:documentation>Configuration parameters of the EULYNX RaSTA redundancy layer. Table 7.3</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="hasCheckCode" nillable="true" type="sig:CheckCodeTypes"/>
      <xs:element name="hasMessagesCounters" nillable="true" type="sig:RedundancyMessageCounters"/>
      <xs:element name="hasTseq" nillable="true" type="rsmCommon:Duration">
        <xs:annotation>
          <xs:documentation>Time period indicating how long a message is buffered that was received outside the sequence (DeferTime, clause 6.6).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="1" name="nPhysicalChannels" nillable="true" type="xs:int">
        <xs:annotation>
          <xs:documentation>Number of physical transport channels (clause 6.1). Default is 2.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="SlabTrack">
    <xs:annotation>
      <xs:documentation>Concrete, ballastless track bed.
de-DE: feste Fahrbahn</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:Trackway">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="isFloating" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>The concrete slab to which the rails are affixed is supported by a flexible layer that attenuates elastic waves.
Need not to be filled in if irrelevant to signalling or unknown.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType abstract="true" name="TcConnector">
    <xs:annotation>
      <xs:documentation>A connector used by track circuits.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:PhysicalTrackAsset">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="1" name="cableType" nillable="true" type="xs:string">
            <xs:annotation>
              <xs:documentation>Description of the type of cable according to cable book. For instance:
&lt;ul&gt;
 &lt;li&gt;DE: 120 Cu, 240 Al. &lt;/li&gt;
 &lt;li&gt;UK: cable type 5.&lt;/li&gt;
 &lt;li&gt;NL: kabel 50 mm2 koper of 70 mm2 aluminium.&lt;/li&gt;
&lt;/ul&gt;</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="connectsToRail" type="generic:LeftRight">
            <xs:annotation>
              <xs:documentation>Denotes to which rail to connect. Given with respect to the orientation of the topological linear element.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="isConnectedToRailAt" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Describes where the TcConnector (or Cable) is connected to the rail, not the dimensions of the connector itself.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>rsmCommon:SpotLocation</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="numberOfStrands" type="xs:int">
            <xs:annotation>
              <xs:documentation>Number of strands in a cable. 
Used in DE.
DE: Verdrillte Seilen. 
NL: aantal aders.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="EtcsBaselines">
    <xs:annotation>
      <xs:documentation>The ETCS baselines.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="B2">
        <xs:annotation>
          <xs:documentation>Baseline 2, also known as SRS 2.3.0d.
</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="B3">
        <xs:annotation>
          <xs:documentation>Baseline 3, which includes Limited Supervision.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="B4">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="SignalAspectDegradation">
    <xs:annotation>
      <xs:documentation>Provides signal aspect degradation information that is associated with a given light signal.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element name="appliesToActiveAspectSet" nillable="true" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Applies to this set of active aspects.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>sig:ActiveAspectSet</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="appliesToLightSignal" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The signal to which the degradation stepping applies.
If no signal is given, the aspect degradation class is a template that can be stored in a file and be applied to signals in particular projects.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>sig:LightSignal</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="appliesWhenAspectFails" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>When this given active aspect fails, degrade to the given active aspect set.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>sig:ActiveAspect</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="degradesToActiveAspectSet" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The active aspect set shown when the commanded set of active aspects can't be shown.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>sig:ActiveAspectSet</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="1" name="isMostRestrictive" nillable="true" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>True: the active aspect set is the most restrictive that the signal shows:
&lt;ul&gt;
 &lt;li&gt;when not controlled by the interlocking, e.g. in case of failed communication between interlocking and signal controller.&lt;/li&gt;
 &lt;li&gt;during system start.&lt;/li&gt;
&lt;/ul&gt;
False: degrade to the given set of active aspects.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="BarrierMechanism">
    <xs:annotation>
      <xs:documentation>The machine that drives the level crossing barrier boom.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element name="appliesToLevelCrossingSystem" nillable="true" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The barrier mechanism belongs to this level crossing system. </xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>sig:LevelCrossingSystem</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="hasBoom" nillable="true" type="sig:BarrierBoom"/>
          <xs:element maxOccurs="1" minOccurs="0" name="hasDistanceToPath" type="rsmCommon:Length">
            <xs:annotation>
              <xs:documentation>Distance between the barrier mechanism and the nearest edge of footway or carriageway that the barrier boom protects.
Only stored if relevant.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="isLocatedAt" nillable="true" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The barrier mechanism is located at this spot location. 
The spot location's application direction is undefined (use quadrant and angle instead).</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>rsmCommon:SpotLocation</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="isPartOfSwitchingGroup" type="xs:int">
            <xs:annotation>
              <xs:documentation>Allocates the barrier mechanism to a group that can be operated or powered by a single switching action.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="isSituatedInQuadrant" type="sig:Quadrants">
            <xs:annotation>
              <xs:documentation>Designates the quadrant or corner where the mechanism is situated (can be added to the designation text).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="usesBearingMountingType" type="sig:BearingMountingTypes">
            <xs:annotation>
              <xs:documentation>Type of mounting of the boom; only filled when not part of design rules or standard arrangement.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="ProceedMessage">
    <xs:annotation>
      <xs:documentation>Indicates that the train may proceed.

Note: the end user can specialise this class to adapt it to national needs.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:AspectSet"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EtcsAcknowledgementMode">
    <xs:annotation>
