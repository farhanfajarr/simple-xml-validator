      <xs:enumeration value="normal">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="reverse">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
