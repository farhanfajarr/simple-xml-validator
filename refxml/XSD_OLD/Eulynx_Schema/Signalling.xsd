      <xs:documentation>Properties of a TPS device.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element name="appliesToTpsDevice" nillable="true" type="rsmCommon:tElementWithIDref">
