  <xs:complexType name="SignalLocation">
    <xs:annotation>
      <xs:documentation>Description of the area where the signal is located, to help maintenance workers to find the signal in the field.
Can be an informal description (e.g. near junction x) or depend on national regulations (e.g. DE Bezeichnung nach Ril 100).
</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="1" name="description" nillable="true" type="xs:string">
            <xs:annotation>
              <xs:documentation>Textual description of the location. Not to be confounded with geo-localisation.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EtcsEndOfAuthority">
    <xs:annotation>
      <xs:documentation>ETCS End Of Authority (EoA) marks the end of an ETCS Movement Authority.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:BaseObject">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="1" name="automaticReleaseSpeedCalculation" nillable="true" type="xs:boolean"/>
          <xs:element name="hasDistanceInRearOfSignal" nillable="true" type="rsmCommon:Length">
            <xs:annotation>
              <xs:documentation>The ETCS EoA is located at a given distance in rear of the signal.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="hasEtcsDangerpoint" type="sig:EtcsDangerpoint">
            <xs:annotation>
              <xs:documentation>Maps to D_DP.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="hasOverlap" type="sig:EtcsOverlap"/>
          <xs:element maxOccurs="1" minOccurs="0" name="hasPermittedSpeed" type="rsmCommon:Speed">
            <xs:annotation>
              <xs:documentation>Speed limit at the EoA.
If not given, the value is considered unknown.
Known as V_EMA in ETCS 3.5.0; formerly known as V_LOA.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="LocalOperatingAreaUiElement">
