                <ObjectTypeRef>sig:EtcsTelegram</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="1" name="sentConditionally" nillable="true" type="sig:EtcsCondition">
