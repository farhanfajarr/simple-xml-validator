            </xs:annotation>
          </xs:element>
          <xs:element name="usesTriggerAssignment" nillable="true" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Link to a trigger assignment that informs about the activation of the Level Crossing </xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>sig:TriggerAssignment</ObjectTypeRef>
              </xs:appinfo>
