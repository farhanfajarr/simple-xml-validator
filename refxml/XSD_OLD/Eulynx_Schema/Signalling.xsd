                <ObjectTypeRef>generic:ReferenceDrawing</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="refersToReferenceDrawingForMarkerBoard" type="rsmCommon:tElementWithIDref">
