              <xs:documentation>The housing of the power supply module. Typically but not necessarily, a LEU cabinet.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>sig:Housing</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="isPsModuleType" type="sig:PowerSupplyModuleType"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="CheckCodeTypes">
    <xs:annotation>
      <xs:documentation>Check code configuration parameter of the redundancy layer (Clause 6.3.6).</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="optionA">
        <xs:annotation>
          <xs:documentation>None</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="optionB">
        <xs:annotation>
          <xs:documentation>CRC32 with polynomial 0x EE5B42FD</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="optionC">
        <xs:annotation>
          <xs:documentation>CRC32 with polynomial 0x 1EDC6F41</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="optionD">
        <xs:annotation>
          <xs:documentation>CRC16 with polynomial 0x 1021</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="optionE">
        <xs:annotation>
          <xs:documentation>CRC16 with polynomial 0x 8005</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="IoController">
    <xs:annotation>
      <xs:documentation>Controller for receiving binary inputs and sending binary outputs.
&lt;ul&gt;
 &lt;li&gt;Inputs, typically received from relay contacts, are packed into telegrams and sent to the interlocking.&lt;/li&gt;
&lt;/ul&gt;
&lt;ul&gt;
 &lt;li&gt;Telegrams received from the interlocking are converted to outputs.&lt;/li&gt;
&lt;/ul&gt;</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:FieldObjectController"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="TpsDeviceProperties">
