              <xs:appinfo>
                <ObjectTypeRef>sig:DiagnosticsCollector</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
