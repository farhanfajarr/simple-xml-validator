            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="1" name="q_gdir" nillable="true" type="xs:int"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="Automatic">
