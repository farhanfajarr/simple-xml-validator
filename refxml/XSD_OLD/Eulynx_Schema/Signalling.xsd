              <xs:appinfo>
                <ObjectTypeRef>sig:ControlledTrackAsset</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
