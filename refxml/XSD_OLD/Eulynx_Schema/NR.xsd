  <xs:complexType name="AwsSystemProperties">
    <xs:annotation>
      <xs:documentation>Class for storing information about the properties of the AWS system.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:TpsDataTransmissionSystemProperties">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="type" type="xs:string">
            <xs:annotation>
              <xs:documentation>General description of the AWS configuration.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="SignalSightingDistance">
    <xs:annotation>
      <xs:documentation>NR Signal sighting distance</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:SignalSightingDistance">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="appliesToSignalFrame" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>for route indicator or shunting signal aspect.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>sig:SignalFrame</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="bannerSignalRequired" type="xs:string">
            <xs:annotation>
              <xs:documentation>In case a repeater is needed, the distance in meter to the banner repeater signal.
Otherwise the value NO is given (hence the use of  type string.)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="hasAchievableDistance" type="rsmCommon:Length">
            <xs:annotation>
              <xs:documentation>Reading distance with the help of mitigation works, e.g. removing obstruction by foliage.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="hasApproachSpeed" type="rsmCommon:Speed">
            <xs:annotation>
              <xs:documentation>Speed of trains approaching the signal if different from the line speed.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="hasCombinedReadableDistance" type="rsmCommon:Length">
            <xs:annotation>
              <xs:documentation>Combined reading distance for the signal and any associated repeaters.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="hasMinimumViewingDistance" type="rsmCommon:Length">
            <xs:annotation>
              <xs:documentation>Smallest distance where the signal can be read. Closer to the signal, the driver may not be able to view the signal.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="hasSupplementaryResponseTime" type="rsmCommon:Duration">
            <xs:annotation>
              <xs:documentation>The supplementary response time for the train driver to interpret the signal information in seconds.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="GridLocation">
    <xs:annotation>
      <xs:documentation>(X,Y) coordinates of the signal frame components as assigned in the Signal Head Editor tool.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element maxOccurs="1" minOccurs="1" name="X" nillable="true" type="xs:int">
        <xs:annotation>
          <xs:documentation>The location of the mounting point on the signal drawing grid; x-coordinate.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="1" name="Y" nillable="true" type="xs:int">
        <xs:annotation>
          <xs:documentation>The location of the mounting point on the signal drawing grid; y-coordinate.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="ReadThroughRiskTypes">
    <xs:annotation>
      <xs:documentation>Is there a read through risk with the signal?</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="nil">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="slight">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="significant">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="TransformerTypes">
    <xs:restriction base="xs:string">
      <xs:enumeration value="booster">
        <xs:annotation>
          <xs:documentation>A transformer that balances traction from overhead line and traction return current for high voltage. A train, or rather the pantograph shouldn't stop where the gap in the overhead line to accommodate the booster transformer is situated.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="auto">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="tractionMountedAuxilary">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="other">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="TpwsDevice">
    <xs:annotation>
      <xs:documentation>TPWS loop. </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:TpsSpotDevice">
        <xs:sequence>
          <xs:element maxOccurs="2" minOccurs="1" name="hasFrequency" nillable="true" type="nr:TpwsFrequencyTypes">
            <xs:annotation>
              <xs:documentation>The arming and the trigger loop of the TSS or OSS is each associated with a frequency.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="positionInGroup" type="xs:int"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="CableBridge">
    <xs:annotation>
      <xs:documentation>A raised construction for guiding cables across an obstacle.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:CableBridge">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="1" name="bridgeNumber" nillable="true" type="xs:string">
            <xs:annotation>
              <xs:documentation>The number of the cable bridge, if used.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
</xs:schema>

