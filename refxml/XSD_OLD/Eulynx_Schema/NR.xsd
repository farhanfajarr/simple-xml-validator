<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:generic="http://dataprep.eulynx.eu/schema/Generic" xmlns:nr="http://dataprep.eulynx.eu/schema/NR" xmlns:rsmCommon="http://www.railsystemmodel.org/schemas/RSM1.2beta/Common" xmlns:sig="http://dataprep.eulynx.eu/schema/Signalling" elementFormDefault="qualified" targetNamespace="http://dataprep.eulynx.eu/schema/NR">
  <!--<dc:date>2021-08-31T12:05:18Z</dc:date><dc:creator>rtm2xsd 1.8.5</dc:creator><dc:source>Model , XMI 2.4.1, EA 13</dc:source>-->
  <xs:import namespace="http://dataprep.eulynx.eu/schema/Generic" schemaLocation="Generic.xsd"/>
  <xs:import namespace="" schemaLocation="Data_container.xsd"/>
  <xs:import namespace="http://dataprep.eulynx.eu/schema/Signalling" schemaLocation="Signalling.xsd"/>
  <xs:import namespace="http://www.railsystemmodel.org/schemas/RSM1.2beta/Common" schemaLocation="Common.xsd"/>
  <xs:complexType name="AtpBeacon">
    <xs:annotation>
      <xs:documentation>A beacon in the ATP system. </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:TpsSpotDevice"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="LightSignal">
    <xs:annotation>
      <xs:documentation>NR Light signal</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:LightSignal">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="hasBackBoard" type="nr:BackBoardTypes"/>
          <xs:element maxOccurs="1" minOccurs="0" name="hasHoodLength" type="nr:HoodsLengthTypes"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="Designation">
    <xs:complexContent>
      <xs:extension base="generic:Designation">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="1" name="longName" nillable="true" type="xs:string"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="LensDiffuserWithHotStrip">
    <xs:complexContent>
      <xs:extension base="nr:LensDiffuser">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="1" name="value" nillable="true" type="xs:string">
            <xs:annotation>
              <xs:documentation>The hot strip indicates the direction of the focus of the light beam and is given in terms like  "left upper quadrant (LUQ)" or "5 o'clock ".</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="OverheadLineMastPairingTypes">
    <xs:restriction base="xs:string">
      <xs:enumeration value="headSpan">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="gantry">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="other">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="RoadTrafficSign">
    <xs:annotation>
      <xs:documentation>Road (traffic) light </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:RoadTrafficSign">
        <xs:sequence>
          <xs:element name="isOfRtlType" nillable="true" type="nr:RtlTypes"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="SignalFrame">
    <xs:annotation>
      <xs:documentation>NR signal frame.
The type of frame is inherent to the aspect, i.e. speed warning, termination, repeater, spate, enhanced permissible speed.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:SignalFrame">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="hasComponent" type="nr:SignalFrameComponent"/>
          <xs:element name="hasFrameDimension" nillable="true" type="nr:FrameDimension"/>
          <xs:element maxOccurs="1" minOccurs="0" name="hasMountingPoint" type="nr:GridLocation"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="TractionFeedTypes">
    <xs:restriction base="xs:string">
      <xs:enumeration value="subStation">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="feederStation">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="trackParallelingHut">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="trackSectioningCabin">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="other">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="LevelCrossingLCU">
    <xs:annotation>
      <xs:documentation>Level crossing Local Control Unit</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:LocalOperationDevice">
        <xs:sequence>
          <xs:element name="isLocatedInQuadrant" nillable="true" type="sig:Quadrants"/>
          <xs:element name="worksLevelCrossingSystem" nillable="true" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>sig:LevelCrossingSystem</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="TpwsSystemProperties">
    <xs:annotation>
      <xs:documentation>TPWS system property set.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:TpsDataTransmissionSystemProperties">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="hasTpwsFunction" type="nr:TpwsFunctionTypes"/>
          <xs:element maxOccurs="unbounded" minOccurs="1" name="hasTpwsTrainCategorySpecificProperties" nillable="true" type="nr:TpwsTrainCategorySpecificProperties"/>
          <xs:element maxOccurs="1" minOccurs="0" name="isOfTpwsSystemType" type="nr:TpwsSystemTypes"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="TrainStop">
    <xs:annotation>
      <xs:documentation>Line-side mechanical device that activates the train's trip cock.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:TpsSpotDevice">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="type" type="xs:string"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="SignalBeyondVisibleTypes">
    <xs:annotation>
      <xs:documentation>Is the signal ahead visible?</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="yes">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="no">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="notApplicable">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="TripcockTester">
    <xs:annotation>
      <xs:documentation>Line-side device for testing the trainborne trip cock.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:TpsSpotDevice">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="type" type="xs:string">
            <xs:annotation>
              <xs:documentation>Informs about the powering.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EquipmentLocation">
    <xs:annotation>
      <xs:documentation>The construct for defining the types of equipment location references able to be located on the infrastructure.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:ConfigurationProperty">
        <xs:sequence>
          <xs:element name="isOfEquipmentLocationType" nillable="true" type="nr:EquipmentLocationTypes"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="AwsMagnet">
    <xs:annotation>
      <xs:documentation>Class for storing information about an AWS magnet.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:TpsSpotDevice">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="positionInGroup" type="xs:int">
            <xs:annotation>
              <xs:documentation>Position in group of the individual AWS magnet.

Increases in the track chainage direction from 1 onwards.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="type" type="xs:string">
            <xs:annotation>
              <xs:documentation>Can inform about
&lt;ul&gt;
 &lt;li&gt;strong magnet&lt;/li&gt;
&lt;/ul&gt;
&lt;ul&gt;
 &lt;li&gt;suppression&lt;/li&gt;
&lt;/ul&gt;</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="LockoutDevice">
    <xs:annotation>
      <xs:documentation>NR device
Patrol man lockout device (PLOD).</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:LockoutDevice">
        <xs:sequence>
          <xs:element name="isOfType" nillable="true" type="nr:LockoutDeviceTypes"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="ApcMagnet">
    <xs:annotation>
      <xs:documentation>Automatic Power Control Magnet.
Strictly speaking, this is not a train protection device but it uses the same technology to transmit information about the neutral section. </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:TpsSpotDevice">
        <xs:sequence>
          <xs:element name="appliesToNeutralSection" nillable="true" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>sig:ElectrificationGap</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="LevelCrossingSystem">
    <xs:annotation>
      <xs:documentation>NR level crossing system.
Note that it is not necessary to define the type or streetname of the protected track crossing, its spot location suffices.
</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:LevelCrossingSystem">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="isFittedWithFloodLight" type="nr:Floodlight">
            <xs:annotation>
              <xs:documentation>The level crossing system is fitted with a headlight unit.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="1" name="isOfLevelCrossingSystemType" nillable="true" type="nr:LevelCrossingControlTypes"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="FullDesignation">
    <xs:annotation>
      <xs:documentation>Used to be the signal box short code. Today, the line code is used.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="nr:Designation">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="1" name="name" nillable="true" type="xs:string"/>
          <xs:element maxOccurs="1" minOccurs="1" name="prefix" nillable="true" type="xs:string"/>
          <xs:element maxOccurs="1" minOccurs="0" name="suffix" type="xs:string">
            <xs:annotation>
              <xs:documentation>Used for special objects like banner or co-acting signals.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="RtlTypes">
    <xs:annotation>
      <xs:documentation>Types of road traffic light.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="msl_pedestrian">
        <xs:annotation>
          <xs:documentation>Miniature stop light, pedestrian
(previously known as miniature warning lights) </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="msl_vehicular">
        <xs:annotation>
          <xs:documentation>Miniature stop light, vehicular
(previously known as miniature warning lights) 
</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="barrowCrossingLights">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="roadTrafficLight">
        <xs:annotation>
          <xs:documentation>Road traffic light; can have a St. Andrews cross</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="GiveWay">
        <xs:annotation>
          <xs:documentation>Give-way sign usually has a St Andrew's cross.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="stAndrewsCrossOnly">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="other">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="TpwsFrequencyTypes">
    <xs:annotation>
      <xs:documentation>TPWS frequencies for Arming and Trigger loops. The loops are stated to be Normal or Opposite Direction.
&lt;ol&gt;
 &lt;li&gt;f1 - 64.25 kHz&lt;/li&gt;
 &lt;li&gt;f2 - 65.25 kHz&lt;/li&gt;
 &lt;li&gt;f3 - 66.25 kHz&lt;/li&gt;
 &lt;li&gt;f4 - 64.75 kHz&lt;/li&gt;
 &lt;li&gt;f5 - 65.75 kHz&lt;/li&gt;
 &lt;li&gt;f6 - 66.75 kHz&lt;/li&gt;
&lt;/ol&gt;</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="f1">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="f2">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="f3">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="f4">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="f5">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="f6">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="f5_f6">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="SignalFrameComponent">
    <xs:annotation>
      <xs:documentation>Lamps or indicators that are part of a signal housing.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:SignalFrameComponent">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="hasGridLocation" type="nr:GridLocation"/>
          <xs:element maxOccurs="1" minOccurs="0" name="pitchAngle" type="xs:double">
            <xs:annotation>
              <xs:documentation>The pitch of the lamp on the component for SSiFT integration</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="yawAngle" type="xs:double">
            <xs:annotation>
              <xs:documentation>The yaw of the lamp on the component for SSiFT integration.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="PopGroup">
    <xs:annotation>
      <xs:documentation>Predicted Overrun Protection (POP) group of signals.
Overrun of any of the signals in the POP group triggers a closing of all signals.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:ConfiguredBaseObject"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="OverheadLineMast">
    <xs:annotation>
      <xs:documentation>NR class for overhead lines carrying mast. 
Also known as stanchion (en-GB).</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="sig:OverheadLineMast">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="isOfPairingType" type="nr:OverheadLineMastPairingTypes">
            <xs:annotation>
              <xs:documentation>Indicates the type of pairing with another mast.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="2" minOccurs="0" name="isPairedWithMast" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Allows pairing a mast with up to 2 other masts across the tracks. </xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>nr:OverheadLineMast</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="stanchionType" type="xs:string"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="TpwsTrainCategorySpecificProperties">
    <xs:sequence>
      <xs:element name="appliesToTrainCategoryForMaximumOverrunDistance" nillable="true" type="rsmCommon:tElementWithIDref">
        <xs:annotation>
          <xs:appinfo>
            <ObjectTypeRef>sig:TrainCategory</ObjectTypeRef>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element name="hasMaximumOverrunDistance" nillable="true" type="rsmCommon:Length"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="SignallingEquivalentUnit">
    <xs:annotation>
      <xs:documentation>Signalling Equivalent Unit (SEU) is used as a way of estimating the size of a signalling project , so this determines if a piece of equipment is counted as an SEU or part of one.
E.g. a signal &amp;amp; a point are 1 SEU each regardless complexity and components.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:ConfigurationProperty">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="1" name="rate" nillable="true" type="xs:float">
            <xs:annotation>
              <xs:documentation>The SEU rate.
Allows weighting the cost of the asset for accounting purposes.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="FrameDimension">
    <xs:sequence>
      <xs:element maxOccurs="1" minOccurs="1" name="isMiniature" nillable="true" type="xs:boolean"/>
      <xs:element maxOccurs="1" minOccurs="1" name="width" nillable="true" type="xs:int">
        <xs:annotation>
          <xs:documentation>Width in mm.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="LockoutDeviceTypes">
    <xs:restriction base="xs:string">
      <xs:enumeration value="p">
        <xs:annotation>
          <xs:documentation>Protects line-side workers, patrol men, by preventing train movement in the wrong direction.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="k">
        <xs:annotation>
          <xs:documentation>Removing the captive key prevents train movements in both directions.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="o">
        <xs:annotation>
          <xs:documentation>Like type "k" except that authorised staff keeps the keys that activate the lockout devices in a lockout area.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="d">
        <xs:annotation>
          <xs:documentation>Disallows main route setting into the lockout area but shunting movements are allowed.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="e">
        <xs:annotation>
          <xs:documentation>Similar to type k except that the key is in possession of platform staff and sometimes siding staff.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="switchLock">
        <xs:annotation>
          <xs:documentation>Old type of lockout device.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="other">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="ConfusionRisk">
    <xs:annotation>
      <xs:documentation>Features that may present confusion risk such as station lights or road lights.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element maxOccurs="1" minOccurs="0" name="backgroundFeatures" type="xs:string"/>
      <xs:element maxOccurs="1" minOccurs="0" name="isReadingThrough" type="nr:ReadThroughRiskTypes"/>
      <xs:element maxOccurs="1" minOccurs="0" name="isSignalAheadVisible" type="nr:SignalBeyondVisibleTypes"/>
      <xs:element maxOccurs="1" minOccurs="0" name="isWithOtherSignals" type="nr:ConfusionRiskWithOtherSignalsTypes"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="PopGroupAssociation">
    <xs:annotation>
      <xs:documentation>Association class describing the conditions applying to individual signals in the POP group.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="generic:BaseObject">
        <xs:sequence>
          <xs:element name="appliesToPopGroup" nillable="true" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>nr:PopGroup</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="appliesToSignal" nillable="true" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>sig:Signal</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="1" name="condition" nillable="true" type="xs:string">
            <xs:annotation>
              <xs:documentation>Note describing the condition.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="hasDerailerElementCondition" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>sig:DerailerElementAndPosition</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="hasPointElementCondition" type="rsmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>sig:PointElementAndPosition</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="LevelCrossingControlTypes">
    <xs:restriction base="xs:string">
      <xs:enumeration value="AHB">
        <xs:annotation>
          <xs:documentation>automatic half barrier crossing</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="ABCL">
        <xs:annotation>
          <xs:documentation>locally-monitored automatic barrier crossing</xs:documentation>
        </xs:annotation>
